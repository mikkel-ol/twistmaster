// 20220501143128
// https://raw.githubusercontent.com/facebook/create-react-app/main/.eslintrc.json

{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "curly": "warn",
    "at-rule-no-unknown": 0,
    "scss/at-rule-no-unknown": 0,
    "no-unused-vars": "off",
    "no-undef": "off"
  }
}
